	</head>
	<body>
		<referenceContainer name="content" >
			<block
				class="JetRails\Cloudflare\Block\Adminhtml\Dashboard"
				template="JetRails_Cloudflare::dashboard.phtml"
			/>
		</referenceContainer>
	</body>
</page>
