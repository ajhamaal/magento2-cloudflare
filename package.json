{
	"version": "1.2.0",
	"name": "magento2-cloudflare",
	"description": "Interact with popular Cloudflare features though Magento's backend portal",
	"author": "Rafael Grigorian",
	"company": "JetRailsÂ®",
	"license": "MIT",
	"namespace": "JetRails_Cloudflare",
	"repository": {
		"type": "git",
		"url": "https://github.com/jetrails/magento2-cloudflare.git"
	},
	"devDependencies": {
		"extract-text-webpack-plugin": "^3.0.2",
		"fs-extra": "^7.0.0",
		"gulp": "^3.9.1",
		"gulp-concat": "^2.6.1",
		"gulp-gzip": "^1.4.2",
		"gulp-magepack": "^1.0.3",
		"gulp-minify": "^2.1.0",
		"gulp-minify-css": "^1.2.4",
		"gulp-replace": "^1.0.0",
		"gulp-sass": "^3.2.1",
		"gulp-tar": "^2.1.0",
		"gulp-uglify": "^3.0.0",
		"npm-run-all": "^4.1.5",
		"webpack": "^3.12.0",
		"webpack-stream": "^4.0.3"
	},
	"dependencies": {
		"jquery": "^3.2.1",
		"jquery-ui": "^1.12.1"
	},
	"scripts": {
		"package-init": "rm -rf dist; mkdir -p dist",
		"package-source": "cp -R src/app/code/JetRails/Cloudflare dist/JetRails_Cloudflare-$npm_package_version;",
		"package-metadata": "cp composer-marketplace.json dist/JetRails_Cloudflare-$npm_package_version/composer.json",
		"package-zip": "cd dist/JetRails_Cloudflare-$npm_package_version; zip -r ../JetRails_Cloudflare-$npm_package_version.zip .",
		"package-clean": "rm -rf dist/JetRails_Cloudflare-$npm_package_version",
		"package": "npm-run-all package-init package-source package-metadata package-zip package-clean"
	}
}
